{
    "name": "edumicro/daisylw4",
    "description": "Agnostic UI components for Laravel, Livewire 4 and DaisyUI",
    "keywords": ["laravel", "livewire", "volt", "daisyui", "tailwind", "autoform"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "edumicro",
            "homepage": "https://github.com/edumicro"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "livewire/livewire": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Edumicro\\DaisyUI\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Edumicro\\DaisyUI\\DaisyServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}